{
  "name": "public",
  "version": "1.0.0",
  "description": "Portfolio website built with requirejs",
  "main": "index.js",
  "scripts": {
    "dev": "http-server",
    "build": "node src/scripts/lib/r.js -o src/scripts/build.js optimize=none",
    "build-public-site": "npm run build && cp src/scripts/require.js dist/scripts/require.js && cp src/index.html dist/index.html && cp -r src/css dist/css && cp -r src/img dist/img"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/aliciakw/aliciano-space.git"
  },
  "author": "aliciakw",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/aliciakw/aliciano-space/issues"
  },
  "homepage": "https://github.com/aliciakw/aliciano-space#readme",
  "dependencies": {
    "requirejs": "^2.3.5"
  }
}
